                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="tItensReceita">
                <xsd:sequence>
                    <xsd:element name="Item" type="tns:tItem" />
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="tItem">
                <xsd:sequence>
                    <xsd:element name="Id" type="xsd:int" />
